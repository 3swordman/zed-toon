inline_arrays:
  comma_separated[3]: a,b,c
  pipe_separated[3]|: x|y|z
  tab_separated[3]	: one	two	three
  mixed_values[5]: 1,true,null,"quoted",unquoted
  empty_array[0]:

tabular_arrays:
  basic_table[3]{id,name,age}:
    1,Alice,30
    2,Bob,25
    3,Charlie,35
  pipe_table[2]|{x,y,z}:
    10|20|30
    40|50|60
  tab_table[2]	{col1,col2,col3}:
    a	b	c
    d	e	f
  mixed_types[3]{id,active,score}:
    1,true,95.5
    2,false,87.3
    3,true,92.1

object_arrays:
  simple_objects[2]:
    - name: Alice
      age: 30
      active: true
    - name: Bob
      age: 25
      active: false
  complex_objects[3]:
    - id: 1
      profile:
        name: Alice Smith
        email: alice@example.com
        verified: true
      scores[3]: 95,87,92
    - id: 2
      profile:
        name: Bob Jones
        email: bob@example.com
        verified: false
      scores[2]: 78,85
    - id: 3
      profile:
        name: Charlie Brown
        email: charlie@example.com
        verified: true
      scores[4]: 88,91,89,93

nested_structures:
  level1:
    level2:
      level3:
        deep_value: found
        deep_array[2]: x,y
      back_to_level2: value
    sibling:
      data: more_data

mixed_array_formats:
  inline_then_nested[1]: first_inline
  nested_then_inline:
    nested_value: inside
  array_with_inline_objects[2]:
    - name: Quick
      value: 1
    - name: Object
      value: 2

delimiter_variations:
  comma_in_header[3,]: a,b,c
  pipe_in_header[3]|: a|b|c
  tab_in_header[3]	: a	b	c
  comma_fields[2]{name,value}:
    Alice,100
    Bob,200
  pipe_fields[2]|{name,value}:
    Alice|100
    Bob|200
  tab_fields[2]	{name,value}:
    Alice	100
    Bob	200

